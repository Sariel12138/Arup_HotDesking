<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.ReleaseBooking">

    <Button
        android:id="@+id/release"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="216dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="23dp"
        android:text="Release Bookings"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="146dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:background="#03A9F4"
        android:text="Email"
        app:layout_constraintBottom_toTopOf="@+id/listview_release"
        app:layout_constraintEnd_toStartOf="@+id/button8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/button8"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:background="#03A9F4"
        android:text="Desk"
        app:layout_constraintBottom_toTopOf="@+id/listview_release"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/button9"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:background="#03A9F4"
        android:text="Minutes Late"
        app:layout_constraintBottom_toTopOf="@+id/listview_release"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/texview_count" />

    <ListView
        android:id="@+id/listview_release"
        android:layout_width="412dp"
        android:layout_height="553dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="195dp"
        android:layout_height="30dp"
        android:layout_marginTop="100dp"
        android:text="Sign-In Time: 8:-0 AM-10: AM"
        android:textColor="#0C0C0C"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toStartOf="@+id/textView18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginEnd="1dp"
        android:text="Current Time: "
        android:textColor="#0C0C0C"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toStartOf="@+id/texview_count"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/release" />

    <TextView
        android:id="@+id/texview_count"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="23dp"
        android:text="     #"
        android:textColor="#0C0C0C"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/release" />

</androidx.constraintlayout.widget.ConstraintLayout>