<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.checkingDateRange">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="296dp"
        android:layout_height="57dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/romanesco"
        android:text="Choose Attempts Report Dates"
        android:textColor="#171717"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_datefromA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_datefromA"
        android:layout_width="256dp"
        android:layout_height="38dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="63dp"
        android:layout_marginEnd="93dp"
        android:layout_marginBottom="50dp"
        android:fontFamily="serif"
        android:hint="From"
        android:textColorHighlight="#3F51B5"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_datetoA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/tv_datetoA"
        android:layout_width="256dp"
        android:layout_height="38dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="93dp"
        android:layout_marginBottom="56dp"
        android:fontFamily="serif"
        android:hint="To"
        android:textColorHighlight="#3F51B5"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_generate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_datefromA" />

    <Button
        android:id="@+id/btn_generate"
        android:layout_width="189dp"
        android:layout_height="41dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="212dp"
        android:background="#DEE2E4"
        android:text="Generate"
        android:textColor="#161617"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_datetoA" />
</androidx.constraintlayout.widget.ConstraintLayout>